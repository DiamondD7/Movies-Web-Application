@model IEnumerable<MovieModel>

<h1 class="text-center">Fitness</h1>

<input type="search" placeholder="Search"/>

<div class="Movie-List">
    @foreach(var i in Model)
    {
        var imageSource = "/images/Fitness/" + i.MovieName + ".jpg";
        <img src="@imageSource" style="width:260px" />
    }
</div>